{
  "author": "Mr_T",
  "background": {
    "scripts": [ "js/jquery-2.2.3.min.js", "js/background.js" ]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "html/popup.html"
  },
  "description": "Message Notification Extension for LinkedIn",
  "manifest_version": 2,
  "name": "Leamon, a message notifier for LinkedIn",
  "permissions": [
    "*://*.linkedin.com/",
    "notifications",
    "tabs",
    "storage",
    "unlimitedStorage",
    "notifications"
  ],
  "version": "1.0"
}